{
	"name": "haxball-cheat-detector",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"python-dev": "pip3 install -r input-analyzer/requirements.txt && cd input-analyzer && uvicorn main:app --reload",
		"svelte-dev": "vite dev",
		"dev": "concurrently \"pnpm run python-dev\" \"pnpm run svelte-dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.3.0",
		"@skeletonlabs/tw-plugin": "^0.2.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@types/pako": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"concurrently": "^8.2.1",
		"jszip": "^3.10.1",
		"pako": "^2.1.0"
	}
}
